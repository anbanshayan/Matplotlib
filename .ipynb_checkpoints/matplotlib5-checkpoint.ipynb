    }
   ],
   "source": [
    "x = np.arange(0,22,2)\n",
    "y = (x*2)+5\n",
    "f1={'color':'red','family':'sans-serif','size':15}\n",
    "plt.plot(x,y)\n",
    "plt.xlabel(\"X axis\",fontdict = f1)\n",
    "plt.ylabel(\"Y axis\",fontdict = f1)\n",
    "plt.show()"
   ]
